{
  "version": "1.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kev-tsx/kev-rcl.git"
  },
  "description": "A rest client library for the pokemon api -> https://pokeapi.co/",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "tsx watch src/test.ts",
    "build": "tsdx build",
    "test": "vitest",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "kev-rcl-tsx",
  "author": "Kev <kevbs.jsx@gmail.com> (https://kevbs.com)",
  "module": "dist/kev-rcl.esm.js",
  "size-limit": [
    {
      "path": "dist/kev-rcl.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/kev-rcl.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/node": "^18.14.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "kev-rcl-tsx": "^1.0.4"
  }
}
